pipeline {
    agent any
    
    stages {
        stage('Build and Package') {
            steps {
                script {
                    sh 'cean package'
                }
            }
        }
        
        stage('Run HelloWorld') {
            steps {
                script {
                    sh 'java -cp target/your-artifact.jar com.coveros.demo.helloworld.HelloWorld'
                }
            }
        }
    }
}
